{
  "name": "tstools",
  "displayName": "TsTools",
  "description": "TypeScript tools. Automatic interface and property implementation.",
  "version": "1.1.3",
  "publisher": "iBotMedia",
  "icon": "images/icon.png",
  "engines": {
    "vscode": "^1.1.0"
  },
  "author": {
    "name": "iBotMedia"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pvantrepote/tstools"
  },
  "bugs": {
    "url": "https://github.com/pvantrepote/tstools/issues"
  },
  "categories": [
    "Other",
    "Snippets"
  ],
  "keywords": [
    "typescript",
    "import",
    "implement",
    "interface",
    "property"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.implement",
        "title": "Implement"
      },
      {
        "command": "extension.import",
        "title": "Import"
      }
    ],
    "keybindings": [
      {
        "command": "extension.implement",
        "key": "ctrl+shift+I",
        "mac": "cmd+shift+I",
        "when": "editorTextFocus"
      },
      {
        "command": "extension.import",
        "key": "ctrl+shift+M",
        "mac": "cmd+shift+M",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.35",
    "@types/node": "^6.0.54",
    "assert": "^1.4.1",
    "mocha": "^3.2.0",
    "typescript": "^2.1.4",
    "vscode": "^1.0.3"
  },
  "dependencies": {
    "fs": "0.0.1-security",
    "path": "^0.12.7",
    "vscode": "^1.0.3"
  }
}
